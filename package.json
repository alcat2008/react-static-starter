{
  "name": "react-static-starter",
  "version": "1.0.0",
  "description": "A React starter project for static website.",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-lite": "^0.15.37"
  },
  "devDependencies": {
    "autoprefixer": "7.1.0",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "css-loader": "0.28.1",
    "dotenv": "4.0.0",
    "enzyme": "^2.9.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "1.7.1",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "fs-extra": "3.0.1",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "2.28.0",
    "jest": "^20.0.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lesshint": "^3.3.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.5",
    "precommit-hook-eslint": "^3.0.0",
    "promise": "7.1.1",
    "react-addons-perf": "^15.4.2",
    "react-dev-utils": "^2.0.0",
    "react-error-overlay": "^1.0.5",
    "react-test-renderer": "^15.5.4",
    "redux-logger": "^2.7.0",
    "style-loader": "0.17.0",
    "sw-precache-webpack-plugin": "0.9.1",
    "url-loader": "0.5.8",
    "webpack": "2.5.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "2.4.5",
    "webpack-manifest-plugin": "1.1.0"
  },
  "scripts": {
    "start": "node bin/start.js",
    "build": "node bin/build.js",
    "test": "node bin/test.js --env=jsdom",
    "eslint": "eslint ./src",
    "eslint-fix": "eslint --fix ./src",
    "lesshint": "lesshint src/styles -r config/lesshint-report.js",
    "lint": "npm run eslint && npm run lesshint",
    "validate": "npm ls",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build --message='feat: Updates gh-pages'"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "modulePathIgnorePatterns": [
      "/_site/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/",
      "/build/"
    ]
  },
  "author": "ge_yangyang@126.com",
  "pre-commit": [
    "lint"
  ]
}
